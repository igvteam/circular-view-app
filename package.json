{
  "name": "igv-jbrowse-circularview",
  "version": "0.1.0",
  "description": "Electron wrapper of jBrowse circular view",
  "main": "src/main/main.js",
  "scripts": {
    "postinstall": "cp node_modules/circular-view/dist/circular-view.js src/renderer/.; electron-builder install-app-deps",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "build": "electron-builder --macos --linux --windows"
  },
  "repository": "https://github.com/igvteam/circ-app",
  "keywords": [
    "genomics"
  ],
  "author": "Jim Robinson",
  "license": "MIT",
  "dependencies": {
    "electron-util": "^0.17.2",
    "new-github-issue-url": "^1.0.0"
  },
  "devDependencies": {
    "electron": "^16.0.8",
    "electron-builder": "^22.14.5",
    "circular-view": "github:igvteam/circular-view",
    "install": "^0.13.0"
  },
  "build": {
    "appId": "igv.org.circview",
    "productName": "IGV JBrowse CircularView",
    "directories": {
      "buildResources": "resources"
    },
    "dmg": {
      "sign": false,
      "iconSize": 160,
      "contents": [
        {
          "x": 180,
          "y": 170
        },
        {
          "x": 480,
          "y": 170,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  }
}
